<main class="profile">
  <section class="user">
    <div class="user__top">
      <img class="user__img" src="{{user.pictureUrl}}" alt="">
      <a href="/edit-profile" >Edit profile</a>
    </div>
    <h1>{{user.username}}</h1>
    <p>{{user.bio}}</p>
    
  </section>
  
  <section class="profile__selector">
    <button id="btn-my-quotes" class="selected">My quotes</button>
    <button id="btn-fav-quotes" class="not-selected">Fav quotes</button>
  </section>
  <section class="profile__my-quotes">
    {{#each quotes}}
      {{>quoteCard this}}
    {{/each}}
  </section>
  <section class="profile__fav-quotes">
    {{#each userFav}}
      {{>quoteCard this}}
    {{/each}}
  </section>
  {{>navbar}}
</main>